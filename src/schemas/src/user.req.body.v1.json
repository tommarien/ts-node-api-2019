{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "firstName": {
      "type": "string",
      "maxLength": 30
    },
    "lastName": {
      "type": "string",
      "maxLength": 80
    },
    "birthDate": {
      "type": "string",
      "format": "date"
    },
    "email": {
      "type": "string",
      "format": "email",
      "maxLength": 120
    }
  },
  "additionalProperties": false,
  "required": ["firstName", "lastName", "email"]
}
